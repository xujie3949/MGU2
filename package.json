{
  "name": "MapSpotterWeb",
  "version": "1.0.0",
  "main": "public/index.html",
  "license": "Apache",
  "scripts": {
    "lint": "better-npm-run lint",
    "lint:fix": "npm run lint -- --fix",
    "dll:dev": "better-npm-run dll-dev",
    "dll:prod": "better-npm-run dll-prod",
    "dev": "better-npm-run dev",
    "compile": "better-npm-run compile",
    "preview": "better-npm-run preview"
  },
  "betterScripts": {
    "lint": {
      "command": "eslint src config --ext .js .styl",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dll-dev": {
      "command": "webpack --config config/webpack.dev.dll.config",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "dll-prod": {
      "command": "webpack --config config/webpack.prod.dll.config",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "webpack-dev-server --config config/webpack.dev.config",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "webpack --config config/webpack.prod.config",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "preview": {
      "command": "http-server ./public -p 8088 -c-1",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "image-webpack-loader": "^3.3.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.8",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "@turf/bbox": "^4.7.3",
    "antd": "^3.0.1",
    "axios": "^0.16.2",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "eventemitter3": "^3.0.0",
    "format-number": "^3.0.0",
    "jsts": "^1.5.0",
    "leaflet": "^1.2.0",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.40.0",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mousetrap": "^1.6.1",
    "node-forge": "^0.7.1",
    "proj4": "^2.4.4",
    "prop-types": "^15.6.0",
    "rc-tween-one": "^1.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "split.js": "^1.3.5",
    "uuid": "^3.1.0"
  }
}
